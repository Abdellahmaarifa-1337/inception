server {
        listen 60 default_server;
        listen [::]:60 default_server;


        root /var/www/html;

        index index.php index.html index.htm index.nginx-debian.html;

        server_name _;

        location / {

                try_files $uri $uri/ =404;
        }

		location ~ /\.ht {
						deny all;
		}

		location = /favicon.ico {
						log_not_found off;
						access_log off;
		}

		location = /robots.txt {
						allow all;
						log_not_found off;
						access_log off;
		}
	
		location ~* \.(js|css|png|jpg|jpeg|gif|ico)$ {
						expires max;
						log_not_found off;
		}
}

