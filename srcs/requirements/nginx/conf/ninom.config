server {
      listen 80;
      listen [::]:80;

    server_name ninom.42.fr;

    index index.html;
	location / {
		proxy_pass http://static_site;
		proxy_redirect 127.0.0.1:80 $scheme://http://static_site/;
	}
	
	location ~* \.(js|css|png|jpg|jpeg|gif|ico)$ {
		proxy_pass http://static_site$uri;
		proxy_redirect http://127.0.0.1:80/ $scheme://http://static_site$uri;
		expires max;
		log_not_found off;
	}

}

